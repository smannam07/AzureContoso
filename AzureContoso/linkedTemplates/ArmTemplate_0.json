					"accessToken": {
						"type": "SecureString",
						"value": "[parameters('ContosoSalesConnection_accessToken')]"
					},
					"newClusterNodeType": "Standard_F4s",
					"newClusterNumOfWorker": "1",
					"newClusterSparkEnvVars": {
